"VertexLitGeneric"
{
	// Original shader: VertexLitTexture
	"$basetexture" "models/jangers/delta_headgear_a_col"
	"$bumpmap" "models/jangers/delta_headgear_a_nml"
	"$phong" "1"
	"$phongboost"   "0.04"
$phongexponent 10
	"$phongfresnelranges" "[0.2 .5 10]"
	"$phongalbedotint" "1"
	"$normalmapalphaenvmapmask" "1"
	"$selfillum" 1

// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "5"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "0.4"		
$nocull 1
}